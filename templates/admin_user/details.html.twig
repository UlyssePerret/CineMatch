<table>
    <thead>
    <tr>
        <th>
            Nom d'utilisateur
        </th>
        <th>
            Email
        </th>
        <th>
            Nom
        </th>
        <th>
            prenom
        </th>
        <th>
            Statut
        </th>
        <th>
            DateNaissance
        </th>
        <th>
            Description
        </th>
        <th>
            Actions
        </th>
    </tr>
    </thead>
    <tbody>
    {% for user in users %}
        <tr>
            <td>
                {{ user.pseudo}}
            </td>
            <td>
                {{ user.email }}
            </td>
            <td>
                {{ user.nom }}
            </td>
            <td>
                {{ user.prenom }}
            </td>
            <td>
                {{ user.statut }}
            </td>
            <td>
                {{ user.datenaissance | date('d/m/Y') }}
            </td>
            <td>
                {{ user.description }}
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
